                        <Button x:Name="SensorIsInsertionBtn" Content="Set" Command="{x:Bind Path=SetSensorIsInsertionCmd}" Grid.Row="5" Grid.Column="2"/>
                        <TextBlock x:Name="Insertion_sA_LODescr" Text="{x:Bind Path=Fields.Insertion_sA_LO.Name}" Grid.Row="6" Margin="10,0,0,0"/>
                        <TextBox x:Name="Insertion_sA_LOVal" Text="{x:Bind Path=Fields.Insertion_sA_LO.ValAsString}" Grid.Row="6" Grid.Column="1" />
                        <Button x:Name="Insertion_sA_LOBtn" Content="Set" Command="{x:Bind Path=SetInsertion_sA_LOCmd}" Grid.Row="6" Grid.Column="2"/>
                        <TextBlock x:Name="Insertion_sB_LODescr" Text="{x:Bind Path=Fields.Insertion_sB_LO.Name}" Grid.Row="7" Margin="10,0,0,0"/>
                        <TextBox x:Name="Insertion_sB_LOVal" Text="{x:Bind Path=Fields.Insertion_sB_LO.ValAsString}" Grid.Row="7" Grid.Column="1" />
                        <Button x:Name="Insertion_sB_LOBtn" Content="Set" Command="{x:Bind Path=SetInsertion_sB_LOCmd}" Grid.Row="7" Grid.Column="2"/>
                        <TextBlock x:Name="Insertion_sC_LODescr" Text="{x:Bind Path=Fields.Insertion_sC_LO.Name}" Grid.Row="8" Margin="10,0,0,0"/>
                        <TextBox x:Name="Insertion_sC_LOVal" Text="{x:Bind Path=Fields.Insertion_sC_LO.ValAsString}" Grid.Row="8" Grid.Column="1" />
                        <Button x:Name="Insertion_sC_LOBtn" Content="Set" Command="{x:Bind Path=SetInsertion_sC_LOCmd}" Grid.Row="8" Grid.Column="2"/>
                        <TextBlock x:Name="Insertion_sD_LODescr" Text="{x:Bind Path=Fields.Insertion_sD_LO.Name}" Grid.Row="9" Margin="10,0,0,0"/>
                        <TextBox x:Name="Insertion_sD_LOVal" Text="{x:Bind Path=Fields.Insertion_sD_LO.ValAsString}" Grid.Row="9" Grid.Column="1" />
                        <Button x:Name="Insertion_sD_LOBtn" Content="Set" Command="{x:Bind Path=SetInsertion_sD_LOCmd}" Grid.Row="9" Grid.Column="2"/>
                        <TextBlock x:Name="Insertion_sA_HIDescr" Text="{x:Bind Path=Fields.Insertion_sA_HI.Name}" Grid.Row="10" Margin="10,0,0,0"/>
                        <TextBox x:Name="Insertion_sA_HIVal" Text="{x:Bind Path=Fields.Insertion_sA_HI.ValAsString}" Grid.Row="10" Grid.Column="1" />
                        <Button x:Name="Insertion_sA_HIBtn" Content="Set" Command="{x:Bind Path=SetInsertion_sA_HICmd}" Grid.Row="10" Grid.Column="2"/>
                        <TextBlock x:Name="Insertion_sB_HIDescr" Text="{x:Bind Path=Fields.Insertion_sB_HI.Name}" Grid.Row="11" Margin="10,0,0,0"/>
                        <TextBox x:Name="Insertion_sB_HIVal" Text="{x:Bind Path=Fields.Insertion_sB_HI.ValAsString}" Grid.Row="11" Grid.Column="1" />
                        <Button x:Name="Insertion_sB_HIBtn" Content="Set" Command="{x:Bind Path=SetInsertion_sB_HICmd}" Grid.Row="11" Grid.Column="2"/>
                        <TextBlock x:Name="Insertion_sC_HIDescr" Text="{x:Bind Path=Fields.Insertion_sC_HI.Name}" Grid.Row="12" Margin="10,0,0,0"/>
                        <TextBox x:Name="Insertion_sC_HIVal" Text="{x:Bind Path=Fields.Insertion_sC_HI.ValAsString}" Grid.Row="12" Grid.Column="1" />
                        <Button x:Name="Insertion_sC_HIBtn" Content="Set" Command="{x:Bind Path=SetInsertion_sC_HICmd}" Grid.Row="12" Grid.Column="2"/>
                        <TextBlock x:Name="Insertion_sD_HIDescr" Text="{x:Bind Path=Fields.Insertion_sD_HI.Name}" Grid.Row="13" Margin="10,0,0,0"/>
