                </Custom:Expander>
                <Custom:Expander x:Name="PulseOutputEXP" Header="Pulse Output" HorizontalAlignment="Stretch" VerticalAlignment="Center" Margin="0,0,0,0" HorizontalContentAlignment="Stretch">
                    <Grid x:Name="PulseOutputGRID">
                        <Grid.RowDefinitions>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
